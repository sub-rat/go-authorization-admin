{
  "name": "go-authorization-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"codegen\"",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint src",
    "lint": "yarn prettier && yarn eslint",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:fix": "eslint --fix src",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "codegen": "openapi-rq -i http://localhost:2222/swagger//doc.json -c axios --output ./src/services/generated"
  },
  "dependencies": {
    "prepare": "husky install",
    "@7nohe/openapi-react-query-codegen": "^0.5.3",
    "@tanstack/match-sorter-utils": "^8.11.8",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "next": "14.1.4",
    "react": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.4.1",
    "@headlessui/tailwindcss": "^0.2.0",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "tailwindcss-animate": "^1.0.7",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "@total-typescript/ts-reset": "^0.5.1",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.3",
    "@openapi-codegen/cli": "^2.0.1",
    "@openapi-codegen/typescript": "^8.0.1",
    "tailwind-variants": "^0.1.20",
    "husky": "^9.0.11",
    "@tailwindcss/forms": "^0.5.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "typescript": "^5"
  },  
  "lint-staged": {
    "!(.next/**/*).{js,ts,jsx,tsx}": [
      "pnpm lint:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}